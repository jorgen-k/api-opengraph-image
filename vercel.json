{
	"functions": {
		"api/og.js": {
			"includeFiles": ""
		}
	},
	"rewrites": [
		{ "source": "/", "destination": "/api/og/" },
		{ "source": "/:url*", "destination": "/api/og/" },
		{ "source": "/:url/", "destination": "/api/og/" },
		{ "source": "/:url/:size/", "destination": "/api/og/" },
		{ "source": "/:url/:size/:format/", "destination": "/api/og/" },
		{ "source": "/:url/:size/:format/:onerror/", "destination": "/api/og/" },
		{ "source": "/:url/:size/:format/:onerror/:cachebust/", "destination": "/api/og/" }
	],
	"trailingSlash": true
}
